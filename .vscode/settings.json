{
    "C_Cpp.errorSquiggles": "disabled",
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
    "editor.minimap.enabled": true,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "files.exclude": {
        "**/.git": true
    },
    "editor.bracketPairColorization.enabled": true,
    "window.zoomLevel": -0.2,
    "workbench.editor.highlightModifiedTabs": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.parameterHints.enabled": true,
    "explorer.compactFolders": false,
    "explorer.decorations.badges": false,
    "explorer.sortOrder": "type",
    "editor.renderWhitespace": "trailing",
    "editor.renderControlCharacters": true,
    "editor.fontFamily": "monospace, Consolas, 'Courier New', monospace",
    "files.associations": {
        "iostream": "cpp",
        "array": "cpp",
        "*.vm": "vm",
        "new": "cpp",
        "optional": "cpp",
        "system_error": "cpp",
        "functional": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "ostream": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "numbers": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp"
    }
